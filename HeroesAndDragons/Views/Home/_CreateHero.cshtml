@*@model HeroesAndDragons.Models.Hero;*@

@*@using HeroesAndDragons.Models;*@
@*@model IEnumerable<Hero>*@

<h1>List of Heroes</h1>
<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Weapon</th>
        <th>Date</th>
    </tr>
    @foreach (var item in ViewBag.Heroess)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Weapon</td>
            <td>@item.Date</td>
        </tr>
    }
</table>
